generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model details {
  id          Int      @id @default(autoincrement())
  name        String
  email       String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
