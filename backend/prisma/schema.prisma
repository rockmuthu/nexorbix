generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model details {
  id          Int      @id @default(autoincrement())
  name        String
  email       String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model contactUs {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  subject   String
  message   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model projectForm {
  id              Int      @id @default(autoincrement())
  name            String
  email           String
  companyName     String
  projectType     String
  others          String?
  estimatedBudget String
  othersTab       String?
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
